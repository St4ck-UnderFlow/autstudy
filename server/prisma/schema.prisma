generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Teacher {
  id   String @id @unique @default(uuid())
  name String
  cpf  String @unique
}

model Student {
  id           String @id @unique @default(uuid())
  name         String
  cpf          String @unique
  supportLevel String
}

model Room {
  id    String @id @unique @default(uuid())
  title String
}