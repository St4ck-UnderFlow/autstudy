generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Teacher {
  id   String @id @unique @default(uuid())
  name String
}

model Student {
  id           String            @id @unique @default(uuid())
  name         String
  supportLevel SupportLevelTypes
}

model Room {
  id    String @id @unique @default(uuid())
  title String
}

enum SupportLevelTypes {
  SLIGHT
  MODERATE
  SEVERE
}
